# AWS Configuration
aws_region     = "us-east-1"
aws_access_key = ""  # Leave empty to use default AWS profile
aws_secret_key = ""

# Project Configuration
project_name = "nexus-ai"
environment  = "dev"

# Feature Flags
enable_sqs            = true
enable_dynamodb       = true
enable_lambda         = true
enable_stepfunctions  = false

# VPC Configuration
# Option 1: Create new VPC (default)
create_vpc = true
vpc_cidr   = "10.0.0.0/16"

# Option 2: Use existing VPC (set create_vpc = false)
# create_vpc = false
# vpc_id     = "vpc-xxxxx"
# subnet_ids = ["subnet-xxxxx", "subnet-yyyyy"]

# DynamoDB Configuration
dynamodb_read_capacity  = 10
dynamodb_write_capacity = 10
